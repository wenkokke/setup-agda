{
  "name": "setup-agda",
  "version": "0.1.0",
  "private": true,
  "description": "Set up a specific version of Agda.",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write '**/*.ts'",
    "format-check": "prettier --check '**/*.ts'",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build --source-map --license licenses.txt",
    "update-agda-info": "ts-node ./src/package-info/update-agda-info.ts",
    "update-haskell-info": "make -q update-haskell-info",
    "all": "npm run build && npm run format && npm run lint && npm run package"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wenkokke/setup-agda.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "Wen Kokke",
  "license": "MIT",
  "dependencies": {
    "@actions/artifact": "^1.1.0",
    "@actions/core": "^1.6.0",
    "@actions/exec": "^1.1.1",
    "@actions/github": "^5.1.1",
    "@actions/glob": "^0.3.0",
    "@actions/io": "^1.1.2",
    "@actions/tool-cache": "^2.0.1",
    "@types/mustache": "^4.2.1",
    "@types/semver": "^7.3.13",
    "mustache": "^4.2.0",
    "object.pick": "^1.3.0",
    "semver": "^7.3.8",
    "setup-haskell": "file:./vendor/haskell/actions/setup"
  },
  "devDependencies": {
    "@types/node": "^18.11.9",
    "@types/object.pick": "^1.3.2",
    "@typescript-eslint/parser": "^5.42.1",
    "@vercel/ncc": "^0.34.0",
    "eslint": "^8.27.0",
    "eslint-plugin-github": "^4.4.1",
    "js-yaml": "^4.1.0",
    "prettier": "2.7.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  }
}
